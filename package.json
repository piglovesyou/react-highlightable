{
  "name": "react-highlightable",
  "version": "0.0.1",
  "description": "Highlightable textarea of React component using contenteditable",
  "main": "./lib/react-highlightable.js",
  "peerDependencies": {
    "react": ">=0.13"
  },
  "author": "thepiglovesyou@gmail.com",
  "license": "Apache",
  "keywords": [
    "react-component",
    "contenteditable",
    "editable",
    "highlight",
    "html",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/piglovesyou/react-highlightable.git"
  },
  "devDependencies": {
    "babel-cli": ">=6",
    "babel-plugin-add-module-exports": ">=0",
    "babel-plugin-transform-object-assign": ">=6",
    "babel-preset-es2015": ">=6"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-assign",
      "add-module-exports"
    ]
  },
  "scripts": {
    "compile": "babel -d lib src",
    "prepublish": "npm run compile"
  },
  "dependencies": {
    "keycode": "^2.1.0",
    "rangy": "^1.3.0"
  }
}
